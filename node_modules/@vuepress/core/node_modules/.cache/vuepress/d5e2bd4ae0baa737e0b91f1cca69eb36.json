 res = []\n  flatten(items, res)\n  for (let i = 0; i < res.length; i++) {\n    const cur = res[i]\n    if (cur.type === 'page' && cur.path === decodeURIComponent(page.path)) {\n      return res[i + offset]\n    }\n  }\n}\n\nfunction flatten (items, res) {\n  for (let i = 0, l = items.length; i < l; i++) {\n    if (items[i].type === 'group') {\n      flatten(items[i].children || [], res)\n    } else {\n      res.push(items[i])\n    }\n  }\n}\n"],"mappings":";AACA,SAASA,WAAW,QAAQ,SAAS;AACrC,OAAOC,QAAQ,MAAM,iBAAiB;AACtC,OAAOC,KAAK,MAAM,cAAc;AAEhC,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE,CAAC,cAAc,CAAC;EACvBC,QAAQ,EAAE;IACRC,IAAIA,CAAA,EAAI;MACN,OAAOC,eAAe,CAACC,UAAU,CAACC,IAAI,EAAE,IAAI,CAAC;IAC/C,CAAC;IAEDC,IAAIA,CAAA,EAAI;MACN,OAAOH,eAAe,CAACC,UAAU,CAACG,IAAI,EAAE,IAAI,CAAC;IAC/C;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAEC,CAAC,EAAE;MAEd,MAAMC,OAAO,GAAGC,QAAQ,CAACC,IAAI,CAACC,WAAW;MACzC,MAAMC,CAAC,GAAGL,CAAC,CAACM,OAAO;MACnB,MAAMC,UAAU,GAAGP,CAAC,CAACQ,MAAM,CAACC,aAAa,CAAC,UAAU,CAAC;MACrD,IAAI,CAACF,UAAU,EAAE;QACf;MACF;MAEA,MAAMG,eAAe,GAAGH,UAAU,CAACI,KAAK;MACxC,IAAIN,CAAC,GAAGJ,OAAO,GAAG,CAAC,EAAE;QACnBS,eAAe,CAACE,KAAK,GAAG,IAAI;QAC5BF,eAAe,CAACG,IAAI,GAAGR,CAAC,GAAG,EAAE,GAAG,IAAI;MACtC,CAAC,MAAM;QACLK,eAAe,CAACG,IAAI,GAAG,IAAI;QAC3BH,eAAe,CAACE,KAAK,GAAGX,OAAO,GAAGI,CAAC,GAAG,EAAE,GAAG,IAAI;MACjD;MACAK,eAAe,CAACI,GAAG,GAAGd,CAAC,CAACe,OAAO,GAAG,EAAE,GAAG,IAAI;IAC7C;EACF;AACF,CAAC;AAED,SAASC,WAAWA,CAAEC,IAAI,EAAEC,KAAK,EAAE;EACjC,OAAOC,IAAI,CAACF,IAAI,EAAEC,KAAK,EAAE,CAAC,CAAC,CAAC;AAC9B;AAEA,SAASE,WAAWA,CAAEH,IAAI,EAAEC,KAAK,EAAE;EACjC,OAAOC,IAAI,CAACF,IAAI,EAAEC,KAAK,EAAE,CAAC,CAAC;AAC7B;AAEA,MAAMxB,UAAU,GAAG;EACjBG,IAAI,EAAE;IACJwB,WAAW,EAAED,WAAW;IACxBE,kBAAkB,EAAEA,CAAC;MAAEC;IAAU,CAAC,KAAKA,SAAS;IAChDC,iBAAiB,EAAEA,CAAC;MAAEC;IAAY,CAAC,KAAKA,WAAW,CAAC7B;EACtD,CAAC;EACDD,IAAI,EAAE;IACJ0B,WAAW,EAAEL,WAAW;IACxBM,kBAAkB,EAAEA,CAAC;MAAEI;IAAU,CAAC,KAAKA,SAAS;IAChDF,iBAAiB,EAAEA,CAAC;MAAEC;IAAY,CAAC,KAAKA,WAAW,CAACjC;EACtD;AACF,CAAC;AAED,SAASC,eAAeA,CACtBkC,QAAQ